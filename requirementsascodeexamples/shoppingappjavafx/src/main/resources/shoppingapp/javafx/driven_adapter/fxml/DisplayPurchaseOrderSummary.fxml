<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="shoppingapp.javafx.controller.DisplayPurchaseOrderSummaryController">
	<children>
		<ToolBar>
			<items>
				<HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
					<children>
						<Button fx:id="finishButton" mnemonicParsing="false"
							onAction="#onFinish" text="Finish" />
					</children>
				</HBox>
			</items>
		</ToolBar>
		<ListView fx:id="purchaseOrderSummaryListView" VBox.vgrow="ALWAYS" />
	</children>
</VBox>
